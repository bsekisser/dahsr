    REM:This is a "Do Nothing" Function!
END SUB

SUB Start
    Flt% = 0
    CLOSE #3: OPEN "dbse\" + Filename$ FOR INPUT AS #3
    IF Ln$ = "" THEN
        INPUT #3, Ln$, Ty$, Px$, Lo$, Hi$
        IF VAL(Ty$) > 2 THEN INPUT #3, Xp$, Xl$, Xh$
        IF Flt% > 0 OR EOF(3) THEN Ln$ = Void$
    ELSE
        LINE INPUT #3, Hd$
    END IF
END SUB

