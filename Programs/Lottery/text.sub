SUB AddLine (c$, i$, j$)
    DIM l$, n%, x%
    IF i$ = "*" AND LEN(j$) > 0 THEN
        l$ = LEFT$(STRING$(MXL / 2 - (LEN(j$) / 2), 32) + j$ + STRING$(MXL, 32), MXL)
    ELSEIF i$ = "É»" AND j$ = "" THEN
        l$ = LEFT$(LEFT$(i$, 1) + STRING$(MXL, 205), MXL - 1) + RIGHT$(i$, 1)
    ELSEIF i$ = "É»" AND LEN(j$) > 0 THEN
        l$ = LEFT$(LEFT$(i$, 1) + STRING$(MXL / 2 - (LEN(j$) / 2) - 2, 205) + "[ " + j$ + " ]" + STRING$(MXL / 2, 205), MXL - 1) + RIGHT$(i$, 1)
    ELSEIF i$ = "È¼" THEN
        IF j$ = "" THEN j$ = Logo$
        l$ = LEFT$(LEFT$(i$, 1) + STRING$(MXL / 2 - (LEN(j$) / 2) - 2, 205) + "[ " + j$ + " ]" + STRING$(MXL / 2, 205), MXL - 1) + RIGHT$(i$, 1)
    ELSEIF i$ = "É" AND j$ = "»" OR i$ = "È" AND j$ = "¼" THEN
        l$ = i$ + STRING$(MXL - 2, 205) + j$
    ELSEIF i$ = "=" AND j$ = "=" THEN
        l$ = "Ì" + STRING$(MXL - 2, 205) + "¹"
    ELSEIF i$ = "-" AND j$ = "-" THEN
        l$ = "Ç" + STRING$(MXL - 2, 196) + "¶"
    ELSEIF i$ = "- " AND j$ = " -" THEN
        l$ = "º " + STRING$(MXL - 4, 45) + " º"
    ELSEIF LEN(i$) > 0 AND j$ = "" THEN
        l$ = LEFT$("º" + STRING$(MXL / 2 - (LEN(i$) / 2), 32) + i$ + STRING$(MXL, 32), MXL - 1) + "º"
    ELSEIF LEN(j$) > 0 AND i$ = "" THEN
        l$ = LEFT$("º" + STRING$(MXL / 2 - (LEN(j$) / 2), 32) + j$ + STRING$(MXL, 32), MXL - 1) + "º"
    ELSEIF LEN(i$) > 0 AND j$ = "*" THEN
        l$ = LEFT$(STRING$(MXL / 2 - (LEN(i$) / 2), 32) + i$ + STRING$(MXL, 32), MXL)
    ELSEIF LEN(i$) > 0 AND LEN(j$) > 0 THEN
        l$ = LEFT$("º " + i$ + STRING$(MXL, 46), MXL - LEN(j$) - 2) + j$ + " º"
    ELSE
        l$ = ""
    END IF
    IF LEN(l$) > 0 THEN
        FOR x% = 1 TO LEN(c$)
        n% = VAL(MID$(c$, x%, 1))
        PRINT #n%, l$
        NEXT
    END IF
END SUB

SUB Cuckoo
    SOUND 1400, 3
    SOUND 0, 2
