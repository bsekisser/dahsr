END SUB

SUB Filter (d$)
    DIM i$, x%
    FOR x% = 1 TO LEN(d$)
    SELECT CASE UCASE$(MID$(d$, x%, 1))
        CASE "0" TO "9": i$ = i$ + MID$(d$, x%, 1)
        CASE ELSE: i$ = i$ + "/"
    END SELECT
    NEXT
    d$ = i$
END SUB

SUB Generate
    DIM g$
    g$ = "0"
    CLOSE
    OPEN "games.txt" FOR INPUT AS #1
    OPEN "text\s0.txt" FOR OUTPUT AS #2
    OPEN "text\h0.txt" FOR OUTPUT AS #8
    AddLine "2", "É»", "Lottery Game Summary Displays"
    AddLine "8", "É»", "Lottery Game History Displays"
    AddLine "28", "Lottery Name", "Filename"
    AddLine "28", "- ", " -"
    WHILE NOT EOF(1)
    Rstv ""
    LINE INPUT #1, Filename$
    Start
    IF Flt% = 0 THEN
        g$ = MID$(STR$(VAL(g$) + 1), 2)
        AddLine "28", RIGHT$(STRING$(LEN(Mxg$), "0") + g$, LEN(Mxg$)) + ":" + Ln$, CHR$(34) + Filename$ + CHR$(34)
        CLOSE #4: OPEN "text\s" + g$ + ".txt" FOR OUTPUT AS #4
        CLOSE #6: OPEN "text\h" + g$ + ".txt" FOR OUTPUT AS #6
        AddLine "4", "É»", "Game #" + RIGHT$(STRING$(LEN(Mxg$), "0") + g$, LEN(Mxg$)) + " Summary Display: " + Today$
        AddLine "6", "É»", "Game #" + RIGHT$(STRING$(LEN(Mxg$), "0") + g$, LEN(Mxg$)) + " History Display: " + Today$
        AddLine "46", "Lottery Name", Ln$
        AddLine "6", "-", "-"
        IF Ln$ = "" OR Ln$ = Void$ THEN
            SkipIt
        ELSE
            Part1
            Part2
        END IF
        IF LEN(Url$) > 0 THEN
            AddLine "46", "-", "-"
            AddLine "46", Url$, ""
        END IF
        AddLine "46", "È¼", ""
    ELSE
        Flt% = 0
    END IF
    WEND
    AddLine "28", "È¼", ""
    CLOSE
END SUB

